{
  "name": "syncai-app",
  "private": true,
  "version": "1.0.0",
  "description": "SyncAI - ChatGPT & Perplexity Integration App",
  "author": "takuy",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev:vite": "vite",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "dev": "concurrently \"npm:dev:vite\" \"npm:dev:electron\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "package": "npm run build && electron-packager . SyncAI --platform=win32 --arch=x64 --out=release --overwrite --ignore=\"src\" --ignore=\"node_modules/.bin\""
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "electron-store": "^11.0.2",
    "openai": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^31.6.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "eslint": "^9.17.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "wait-on": "^8.0.1"
  }
}
